<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.web.WebView?>
<GridPane xmlns:fx="http://javafx.com/fxml"
          fx:controller="com.example.demo.controller.SampleController" alignment="center" hgap="10" vgap="10"
          stylesheets="@css/style.css" fx:id="gridPane">

    <Button text="Open..." onAction="#handleClick" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
    <Button text="Two" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
    <Button text="Three" GridPane.rowIndex="0" id="blue-button" GridPane.columnIndex="2"/>
    <Button text="Four" fx:id="button4" GridPane.rowIndex="0" GridPane.columnIndex="3"/>
    <Button text="Five" GridPane.rowIndex="0" GridPane.columnIndex="4"/>

    <Label text="Example" fx:id="label" onMouseEntered="#handleMouseEnter" onMouseExited="#handleMouseExit"
           GridPane.rowIndex="1" GridPane.columnIndex="0"/>

    <Hyperlink text="www.javafx.com" GridPane.rowIndex="3" GridPane.columnIndex="0" onAction="#handleLinkClick"/>

    <WebView fx:id="webView" GridPane.rowIndex="4" GridPane.columnIndex="0" GridPane.columnSpan="5" GridPane.rowSpan="3"/>

</GridPane>
